{
  "name": "sanity-nextjs-vercel-example",
  "version": "1.0.0",
  "private": true,
  "description": "A bare bones example of a Vercel-deployable project with a Next.js frontend and a Sanity Studio on /studio",
  "devDependencies": {
    "concurrently": "^5.3.0",
    "lerna": "^3.22.1"
  },
  "scripts": {
    "dev": "concurrently \"next --port $PORT\" \"cd admin && sanity start\"",
    "build": "echo 'Building Sanity to public/studio' && cd admin && sanity build ../public/admin -y && cd .. && next build",
    "start": "vercel dev"
  },
  "keywords": [],
  "author": "Sanity.io <hello@sanity.io>",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@portabletext/react": "^1.0.6",
    "@sanity/cli": "^2.0.5",
    "@sanity/client": "^3.3.0",
    "@sanity/image-url": "^1.0.1",
    "next": "^12.0",
    "react": "^17",
    "react-dom": "^17",
    "@sanity/base": "^2.29.5",
    "@sanity/core": "^2.29.5",
    "@sanity/default-layout": "^2.29.7",
    "@sanity/default-login": "^2.29.5",
    "@sanity/desk-tool": "^2.29.7",
    "@sanity/eslint-config-studio": "^2.0.0",
    "@sanity/vision": "^2.29.5",
    "eslint": "^8.6.0",
    "prop-types": "^15.7",
    "react": "^17.0",
    "react-dom": "^17.0",
    "styled-components": "^5.2.0"
  }
}
